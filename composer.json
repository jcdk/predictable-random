{
    "name": "jcdk/predictable-random",
    "description": "Generates a predictable random number based on a starting seed.",
    "type": "craft-plugin",
    "version": "1.0.8",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "predictable-random"
    ],
    "support": {
        "docs": "https://github.com/jcdk/predictable-random",
        "issues": "https://github.com/jcdk/predictable-random/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Toft",
            "homepage": "https://www.hai-studio.com"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0"
    },
    "repositories": [
        {
            "packagist.org": true
        }
    ],
    "autoload": {
        "psr-4": {
          "haistudio\\predictablerandom\\": "src/"
        }
    },
    "extra": {
        "name": "Predictable Random",
        "handle": "predictable-random",
        "schemaVersion": "1.0.1",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://www.hai-studio.com"
    }
}
